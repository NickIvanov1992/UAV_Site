@model ServiceItem
@{
    ViewBag.Title = Model.MetaTitle;
    ViewBag.Description = Model.MetaDescription;
    ViewBag.Keywords = Model.MetaKeywords;
}

<div>
    <h2>
        @Model.Title
    </h2>
    <h2>
        @Model.Subtitle
    </h2>
    <div>
        <img class="image-centered" src="~/images/@Model.TitleImagePath" width="800px" />
    </div>
</div>
<div>
    @Html.Raw(Model.Text)
</div>